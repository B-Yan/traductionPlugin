{
	"_comment1": "Infos about extension"
	"manifest_version": 2,
	"name": "AudioSearch",
	"version": "0.1",
	"description": "Parse audio content to get input timestamp",
	"browser_action": {
		"default_icon": 'icon.png,
		"default_popup": popup.html
	}
	"permissions": ["activeTab"]
	
		"content_scripts":[
		{
			"matches":[
			 "<all_urls>"
			 ],
			 "js": ["content.js]
		}
	  ]
}
