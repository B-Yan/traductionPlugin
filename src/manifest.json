{
	"manifest_version": 3,
	"name": "AudioSearch",
	"version": "0.1",
	"description": "Parse audio content to get input timestamp",
	"action": {
		"default_icon": "frontend/icon.png",
		"default_popup": "frontend/popup.html",
		"default_title": "AudioSearch"
	},
	"permissions": ["activeTab", "storage"],
	"content_scripts":[
	{
		"matches":[
		 "<all_urls>"
		 ],
		"js": ["frontend/content.js"]
	}
	]
}
